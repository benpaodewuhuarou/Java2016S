import java.awt.*;

public class Bus {
	private int speed;
	private int numPassengers;

	public Bus(int speed, int n) { this.speed = speed; numPassengers = n; }
	public String toString() {
		return "I'm a bus! speed=" + speed + " numPassengers=" + numPassengers;
	}
	public void payToll() {
		System.out.println("pay $3");
	}										
}
